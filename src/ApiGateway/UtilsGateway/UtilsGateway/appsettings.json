{
    "ReverseProxy": {
        "Routes": {
            "usersRoute": {
                "ClusterId": "usersCluster",
                "Match": {
                    "Path": "/users/{**catch-all}"
                }
            },
            "pdfRoute": {
                "ClusterId": "pdfCluster",
                "Match": {
                    "Path": "/pdf/{**catch-all}"
                }
            }
        },
        "Clusters": {
            "usersCluster": {
                "Destinations": {
                    "usersApi": { "Address": "http://utilsuserservice:5001" }
                }
            },
            "pdfCluster": {
                "Destinations": {
                    "pdfApi": { "Address": "http://utilspdfservice:5002" }
                }
            }
        }
    }
}
